    "@eslint/js": "^9.4.0",
    "@nestjs/cli": "^10.3.2",
    "@typescript-eslint/parser": "^7.13.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "husky": "^9.0.11",
    "npm-check": "^6.0.1",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "^20.12.2",
    "npm": "^10.5.0",
    "yarn": "^1.22.19"
  }
