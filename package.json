  "scripts": {
    "build": "nest build",
    "check-libs": "npm-check",
    "check-not-used-libs": "depcheck",
    "format": "prettier --write \"src/**/*.ts\" validator-env-example.ts",
    "lint": "eslint \"src/**/*.ts\" validator-env-example.ts --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/applications/main",
    "validate-env-example": "tsc validator-env-example.ts && node validator-env-example.js; EXIT_CODE=$?; rm -rf validator-env-example.js src/applications/env.schema.js; exit $EXIT_CODE",
    "validate-format": "prettier \"src/**/*.ts\" validator-env-example.ts --check",
    "validate-lint": "eslint \"src/**/*.ts\" validator-env-example.ts "
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "class-transformer": "^0.5.1",
    "cron": "^3.1.7",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "mongoose": "^8.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "uuid": "^10.0.0"
  },
